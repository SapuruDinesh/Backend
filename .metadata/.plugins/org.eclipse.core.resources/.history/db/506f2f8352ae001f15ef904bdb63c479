package com.backend;

public class Paintwall {
    public static void main(String[] args) {
        String[] pointStrings = args[0].split("\\s+");
        int n = pointStrings.length / 2;
        int[][] points = new int[n][2];
        for (int i = 0; i < n; i++) {
            points[i][0] = Integer.parseInt(pointStrings[2 * i]);
            points[i][1] = Integer.parseInt(pointStrings[2 * i + 1]);
        }

        int m = Integer.parseInt(args[1]);

        int xMin = Integer.MAX_VALUE, yMin = Integer.MAX_VALUE, xMax = Integer.MIN_VALUE, yMax = Integer.MIN_VALUE;
        for (int[] point : points) {
            xMin = Math.min(xMin, point[0]);
            yMin = Math.min(yMin, point[1]);
            xMax = Math.max(xMax, point[0]);
            yMax = Math.max(yMax, point[1]);
        }

        int rows = (yMax - yMin + m - 1) / m;
        int cols = (xMax - xMin + m - 1) / m;

        System.out.println(rows * cols);
    }
}
